{
  "title": {
    "type": "string",
    "source": "html",
    "selector": ".smb-pricing-table__item__title",
    "default": ""
  },
  "price": {
    "type": "string",
    "source": "html",
    "selector": ".smb-pricing-table__item__price",
    "default": ""
  },
  "lede": {
    "type": "string",
    "source": "html",
    "selector": ".smb-pricing-table__item__lede",
    "default": ""
  },
  "list": {
    "type": "string",
    "source": "html",
    "selector": "ul",
    "multiline": "li",
    "default": ""
  },
  "btnLabel": {
    "type": "string",
    "source": "html",
    "selector": ".smb-pricing-table__item__btn > .smb-btn__label",
    "default": ""
  },
  "btnURL": {
    "type": "string",
    "source": "attribute",
    "selector": ".smb-pricing-table__item__btn",
    "attribute": "href",
    "default": ""
  },
  "btnTarget": {
    "type": "string",
    "default": "_self"
  },
  "btnBackgroundColor": {
    "type": "string"
  },
  "btnTextColor": {
    "type": "string"
  },
  "imageID": {
    "type": "number",
    "default": 0
  },
  "imageURL": {
    "type": "string",
    "source": "attribute",
    "selector": ".smb-pricing-table__item__figure > img",
    "attribute": "src",
    "default": ""
  },
  "imageAlt": {
    "type": "string",
    "source": "attribute",
    "selector": ".smb-pricing-table__item__figure > img",
    "attribute": "alt",
    "default": ""
  },
  "imageSizeSlug": {
    "type": "string",
    "default": "large"
  }
}
